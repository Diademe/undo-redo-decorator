        "prettier": "^1.14.3",
        "ts-jest": "^23.10.4",
        "ts-node": "^7.0.1",
        "tslint": "^5.10.0",
        "tslint-eslint-rules": "^5.4.0",
        "typescript": "^3.1.2"
    },
    "jest": {
        "testRegex": "/spec/.*\\.spec\\.([tj]sx?)$",
        "transform": {
            "^.+\\.[jt]sx?$": "ts-jest"
        },
        "moduleFileExtensions": [
            "ts",
            "tsx",
            "js",
            "jsx"
        ],
        "collectCoverageFrom": [
            "src/**/*.ts",
            "spec/**/*.ts"
        ],
        "coverageDirectory": "."
    },
    "dependencies": {
        "typescript": "^2.8.3"
    }
